    <PackageTags>blazor icons material materialdesign google</PackageTags>
  </PropertyGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\..\Leeax.Web.Components\Leeax.Web.Components.csproj" />
  </ItemGroup>

</Project>